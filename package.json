{
  "name": "notdown",
  "description": "Markdown note taking app built with Next.js and Prisma",
  "license": "UNLICENSED",
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.80",
    "@mui/material": "^5.6.2",
    "@prisma/client": "3.4.2",
    "@svgr/webpack": "^6.2.1",
    "@uiw/react-markdown-editor": "^4.0.3",
    "apollo-server-core": "^3.6.7",
    "apollo-server-micro": "^3.6.7",
    "bcryptjs": "^2.4.3",
    "graphql": "^16.4.0",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "next": "latest",
    "next-remove-imports": "^1.0.6",
    "nexus": "^1.3.0",
    "nexus-prisma": "^0.35.0",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "^7.30.0",
    "react-spinners": "^0.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-codegen/typescript-react-apollo": "^3.2.11",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "16.11.7",
    "@types/react": "latest",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "prisma": "3.4.2",
    "ts-node": "^10.7.0",
    "typescript": "latest"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "scripts": {
    "init": "prisma db push",
    "dev": "next dev",
    "build": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' yarn generate && next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "yarn generate:prisma && yarn generate:nexus && yarn generate:graphql",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' schema",
    "generate:graphql": "graphql-codegen --config codegen.js"
  }
}
