{
  "name": "notdown",
  "description": "Markdown note taking app built with Next.js and Prisma",
  "license": "UNLICENSED",
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.2",
    "@uiw/react-markdown-editor": "^4.0.3",
    "next": "latest",
    "next-remove-imports": "^1.0.6",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@types/react": "latest",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "prisma": "3.4.2",
    "typescript": "latest"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "scripts": {
    "init": "prisma db push && prisma db seed",
    "dev": "next dev",
    "build": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' yarn generate:prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "generate:prisma": "prisma generate"
  }
}
