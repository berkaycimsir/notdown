generator client {
  provider = "prisma-client-js"
  previewFeatures = ["dataproxy"]
}

generator nexusPrisma {
  provider = "nexus-prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  username String @unique
  fullName String
  password String 
}